            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/TransferBetweenAccounts"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AccountVindi": {
        "type": "object",
        "properties": {
          "nomeCliente": {
            "type": "string",
            "nullable": true
          },
          "documento": {
            "type": "string",
            "nullable": true
          },
          "personType": {
            "$ref": "#/components/schemas/PersonType"
          },
          "dataAberturaConta": {
            "type": "string",
            "format": "date-time"
          },
          "statusConta": {
            "type": "string",
            "nullable": true
          },
          "formattedDocument": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          },
          "saldo": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "DeleteAccount": {
        "type": "object",
        "properties": {
          "documento": {
            "type": "string",
            "nullable": true
          },
          "formattedDocument": {
            "type": "string",
            "nullable": true,
            "readOnly": true
          }
        },
        "additionalProperties": false
      },
      "PersonType": {
        "enum": [
          0,
          1
        ],
        "type": "integer",
        "format": "int32"
      },
      "TransferBetweenAccounts": {
        "type": "object",
        "properties": {
          "documentoEntrada": {
            "type": "string",
            "nullable": true
          },
          "documentoSaida": {
            "type": "string",
            "nullable": true
          },
          "statusConta": {
            "type": "boolean"
          },
          "saldoDocumentoEntrada": {
            "type": "number",
            "format": "double"
          },
          "saldoDocumentoSaida": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      }
