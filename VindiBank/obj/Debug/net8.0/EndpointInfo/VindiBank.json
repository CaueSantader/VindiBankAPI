{
  "openapi": "3.0.4",
  "info": {
    "title": "VindiBank",
    "version": "1.0"
  },
  "paths": {
    "/api/controller/CreateAccount": {
      "post": {
        "tags": [
          "Home"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AccountVindi"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/AccountVindi"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/AccountVindi"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/controller/GetAccounts": {
      "get": {
        "tags": [
          "Home"
        ],
        "parameters": [
          {
            "name": "documento",
            "in": "query",
            "schema": {
              "type": "string",
              "default": ""
            }
          },
          {
            "name": "nomeCliente",
            "in": "query",
            "schema": {
              "type": "string",
              "default": ""
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/controller/AccountDeactivation": {
      "delete": {
        "tags": [
          "Home"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DeleteAccount"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/DeleteAccount"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/DeleteAccount"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/controller/AccountTransfer": {
      "put": {
        "tags": [
          "Home"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TransferBetweenAccounts"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TransferBetweenAccounts"
